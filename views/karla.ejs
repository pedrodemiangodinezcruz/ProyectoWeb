<body>
	<button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#fallido">
		Launch demo modal
	  </button>
<% if(success == 'false'){%>
	
	<div class="modal fade" id="fallido">
		<div class="modal-dialog" role="document">
		  <div class="modal-content">
			<div class="modal-header">
			  <h5 class="modal-title" id="exampleModalLabel">Licencias invalidas</h5>
			  <form method="GET" action="/consultaExpedientes">
				<button type="button" class="btn-close" data-bs-dismiss="fallido" aria-label="Close"></button>
			  </button>
			</form>
			</div>
			<div class="modal-body">
				<div class="alert alert-danger" role="alert">
					Email o Password invalidos<a href="/" class="alert-link"> OK </a>
				  </div>
			</div>
		  </div>
		</div>
	  </div>
	  <script>
		$('#fallido').modal('show');
	</script>
	  <% }%>
<!-- Button trigger modal -->


</body>
</html>
